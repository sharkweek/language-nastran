    file_management:
        captures:
            "1":
                name: "keyword.file_management.nastran"
        match:
            "(ACQUIRE|ASSIGN|CONNECT|DBCLEAN|DBDIC|DBDICT|DBDIR|DBFIX|DBLOAD|DBLOCATE|DBSETDEL|DBUNLOAD|DBUPDATE|DEFINE|ENDJOB|EXPAND|INCLUDE|INIT|NASTRAN|PROJ|PROJECT|RESTART|RFINCLUDE)"
