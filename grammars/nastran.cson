name: "NASTRAN"
scopeName: "source.nastran"
fileTypes: [
    "bdf"
    "dat"
    "nas"
]
patterns: [
    {
        name: "comment.line.character.nastran"
        captures:
            "1":
                name: "comment.line.character.nastran"
        match: "(\\$).*$\\n?"
    }
    {
        name: "string.quoted.single.nastran"
        comment: "single quoted string (does not allow interpolation)"
        begin: "'"
        end: "'"
        beginCaptures:
            "0":
                name: "punctuation.definition.string.begin.nastran"
        endCaptures:
            "0":
                name: "punctuation.definition.string.end.nastran"
        patterns: [
            {
                name: "constant.character.escape.nastran"
                match: "\\\\'|\\\\\\\\"
            }
        ]
    }
    {
        begin: "\""
        name: "string.quoted.double.untitled"
        patterns: [
        {
            name: "constant.character.escape.nastran"
            match: "//."
        }
        ]
        end: "\""
    }
    {
        name: "string.quoted.double.nastran"
        begin: "'"
        end: "'"
        comment: "single quoted string (does not allow interpolation)"
        beginCaptures:
            "0":
                name: "punctuation.definition.string.begin.nastran"
        endCaptures:
            "0":
                name: "punctuation.definition.string.end.nastran"
        patterns: [
            {
                  name: "constant.character.escape.nastran"
                  match: "\\\\\"|\\\\\\\\"
            }
        ]
    }
    {
        include: "#file_management"
    }
    {
        include: "#executive_control"
    }
    {
        include: "#case_control"
    }
    {
        include: "#plot"
    }
    {
        include: "#parameters"
    }
    {
        include: "#bulk_data_constraints"
    }
    {
        include: "#bulk_data_elements"
    }
    {
        include: "#bulk_data_geometry"
    }
    {
        include: "#bulk_data_loads"
    }
    {
        include: "#bulk_data_materials"
    }
    {
        include: "#bulk_data_misc"
    }
]
